#CRIA A NETWORK
docker network create tagid-network

#BUILD
docker build --no-cache --tag moyseszerbieti/tagid . (a partir do Dokcerfile)

#RUN DAS IMAGENS DO POSTGRES E DA APLICAÇÃO DENTRO DA NETWORK
docker run
--name tagid-db
--network tagid-network
-d
-e POSTGRES_PASSWORD=(senha)
-e POSTGRES_USERNAME=(login)
-e POSTGRES_DB=(nome do banco)
-p 5433:5432
-d postgres:16.9

docker run --name tagid-prod -d -p 8080:8080 (nome da imagem)